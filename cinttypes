// clang-format off
#pragma once

#include "abc.hpp"

// All macros are defined in `abc.hpp`

#ifndef ywstd_import
ywstd_export namespace std {
inline intmax_t __cdecl imaxabs(intmax_t n) { return ::imaxabs(n); }
inline imaxdiv_t __cdecl imaxdiv(intmax_t num, intmax_t den) { return ::imaxdiv(num, den); }
inline intmax_t __cdecl strtoimax(const char* str, char** endptr, int base) { return ::strtoimax(str, endptr, base); }
inline intmax_t __cdecl wcstoimax(const wchar_t* str, wchar_t** endptr, int base) { return ::wcstoimax(str, endptr, base); }
inline uintmax_t __cdecl strtoumax(const char* str, char** endptr, int base) { return ::strtoumax(str, endptr, base); }
inline uintmax_t __cdecl wcstoumax(const wchar_t* str, wchar_t** endptr, int base) { return ::wcstoumax(str, endptr, base); }
}
#endif
