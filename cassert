#pragma once

#if defined(ywstd_export) and __has_include("inc/cassert")
#include "inc/cassert"
namespace yw::_ {
export inline void wassert(const wchar* Message, const wchar* File, unsigned Line) { ::_wassert(Message, File, Line); }
}
#else

namespace yw::_ {

/// internal function, used by `yw::assert`
void wassert(const wchar* Message, const wchar* File, unsigned Line);
}
#endif
